@model IEnumerable<SaleAppMVC.Models.paytype>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.paytype_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.paytype_name)
        </th>
        <th>操作</th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.paytype_id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.paytype_name)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.paytype_id }) |
        @Html.ActionLink("Details", "Details", new { id = item.paytype_id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.paytype_id })
    </td>
</tr>
}

</table>
